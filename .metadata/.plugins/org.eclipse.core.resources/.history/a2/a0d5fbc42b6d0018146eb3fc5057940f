package calc;

public class Double {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double a = new java.util.Scanner(System.in).nextDouble();
		double b = new java.util.Scanner(System.in).nextDouble();
		double c = new java.util.Scanner(System.in).nextDouble();
		
		System.out.println(getAverage(a,b,c));
	}
	
	public static double getAverage(double a,double b,double c) {
		double res;
		res = (a+b+c)/3;
		return res;
	}

}
